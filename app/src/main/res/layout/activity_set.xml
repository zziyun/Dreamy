<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SetActivity">

    <LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_alignParentTop="true"
    android:layout_marginTop="0dp"
    android:orientation="vertical">

    <include layout="@layout/toolbar_set" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:gravity="center">

        <Button
            android:id="@+id/btn_set_modify5"
            style="?android:attr/borderlessButtonStyle"
            android:layout_width="141dp"
            android:layout_height="50dp"
            android:layout_marginTop="25dp"
            android:background="@drawable/border_fill"
            android:text="개인 정보 수정"
            android:textColor="#FFFFFF"
            android:textSize="14dp"
            android:textStyle="bold" />

        <Button
            android:id="@+id/btn_set_logout5"
            style="?android:attr/borderlessButtonStyle"
            android:layout_width="141dp"
            android:layout_height="50dp"
            android:layout_marginTop="25dp"
            android:background="@drawable/border_fill"
            android:text="로그아웃"
            android:textColor="#FFFFFF"
            android:textSize="14dp"
            android:textStyle="bold" />

        <Button
            android:id="@+id/btn_set_data5"
            style="?android:attr/borderlessButtonStyle"
            android:layout_width="141dp"
            android:layout_height="50dp"
            android:layout_marginTop="25dp"
            android:background="@drawable/border_fill"
            android:text="데이터 초기화"
            android:textColor="#FFFFFF"
            android:textSize="14dp"
            android:textStyle="bold" />

        <Button
            android:id="@+id/btn_set_delete5"
            style="?android:attr/borderlessButtonStyle"
            android:layout_width="141dp"
            android:layout_height="50dp"
            android:layout_marginTop="25dp"
            android:background="@drawable/border_fill"
            android:text="회원탈퇴"
            android:textColor="#FFFFFF"
            android:textSize="14dp"
            android:textStyle="bold" />

    </LinearLayout>
</RelativeLayout>